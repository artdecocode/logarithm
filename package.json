{
  "name": "logarithm",
  "version": "2.0.1",
  "description": "The Middleware For Web-Server To Record Hits In ElasticSearch.",
  "main": "compile/index.js",
  "module": "src/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec test/mask",
    "spec": "yarn t test/spec",
    "mask": "yarn t test/mask",
    "test-build": "ALAMODE_ENV=test-build yarn test",
    "lint": "eslint .",
    "doc": "NODE_DEBUG=doc doc -o README.md -n _logarithm",
    "dev": "node src/bin",
    "b": "alamode src -o build -i bin -s",
    "d": "yarn-s template d1 externs",
    "d1": "typal compile/index.js -u -t types/api.xml,types/index.xml",
    "template": "typal compile/template.js -T compile/index.js -t types/api.xml,types/index.xml",
    "externs": "typal types/externs.js",
    "build": "yarn-s d b doc",
    "lib": "depack src/depack -c -o compile/logarithm.js -a -p -s",
    "compile": "depack src/bin/logarithm.js -c -o compile/bin -a -p -w -S -s",
    "logarithm": "node compile/bin/logarithm.js $ELASTIC:9200",
    "rec": "NODE_DEBUG=appshot appshot -T 23 -a Terminal -y 150 -f",
    "args": "argufy types/args/* -o src/bin/get-args.js",
    "e": "alanode example",
    "example/": "yarn e example"
  },
  "files": [
    "compile",
    "src",
    "types/externs.js"
  ],
  "externs": "types/externs.js",
  "bin": {
    "logarithm-dev": "src/index.js",
    "logarithm": "compile/bin/logarithm.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/artdecocode/logarithm.git"
  },
  "keywords": [
    "logarithm",
    "log",
    "logs",
    "koa",
    "elastic",
    "es",
    "elasticsearch",
    "elastic search",
    "kibana"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artdecocode/logarithm/issues"
  },
  "homepage": "https://github.com/artdecocode/logarithm#readme",
  "devDependencies": {
    "@demimonde/dotenv": "^1.1.0",
    "@externs/goa": "^0.1.1",
    "@idio/idio": "^0.2.1",
    "@rqt/aqt": "^1.3.0",
    "@typedefs/goa": "^0.0.6",
    "alamode": "^3.2.0",
    "argufy": "^1.7.5",
    "catchment": "^3.3.0",
    "depack": "^1.0.1",
    "documentary": "^1.34.2",
    "erotic": "^2.1.1",
    "erte": "^1.2.0",
    "eslint-config-artdeco": "1.0.1",
    "indicatrix": "^1.4.0",
    "reloquent": "^1.3.2",
    "rqt": "^4.0.0",
    "tablature": "^1.2.0",
    "usually": "^1.2.0",
    "yarn-s": "^1.2.1",
    "zoroaster": "^4.1.2"
  },
  "dependencies": {}
}
