{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Zoroaster",
      "program": "${workspaceFolder}/node_modules/.bin/zoroaster",
      "env": {
        "NODE_DEBUG": "logarithm",
        "ALAMODE_ENV": "test-compile",
      },
      "console": "integratedTerminal",
      "args": [
        "test/spec",
        "test/mask",
        "-a",
        "-w",
        "-t",
        "9999999"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Example",
      "program": "${workspaceFolder}/node_modules/.bin/alanode",
      "env": {
        "ELASTIC": "http://192.168.1.2:9200",
        "NODE_DEBUG": "logarithm",
      },
      "console": "integratedTerminal",
      "args": [
        "example"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Logarithm",
      "program": "${workspaceFolder}/src/bin/index.js",
      "env": {
        "NODE_DEBUG": "logarithm",
      },
      "console": "integratedTerminal",
      "args": [
        "172.31.12.175:9200",
        "-r", "hello", "-s", "test"
        // "-p", "info2",
        // "-rp", "info2",
        // "-P",
        // "-t", "clients"
        // "-T",
        // "-rp", "info2",
        // "-S",
      ],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Depack",
      "program": "${workspaceFolder}/depack/logarithm.js",
      "env": {
        "NODE_DEBUG": "logarithm",
      },
      "console": "integratedTerminal",
      "args": [
        "$ELASTIC:9200",
        // "-p", "info2",
        // "-rp", "info2",
        // "-P",
        // "-t", "clients"
        // "-T",
        "-rp", "info2"
        // "-d", "clients-*"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    }
  ]
}
